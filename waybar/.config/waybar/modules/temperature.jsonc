{
  "temperature": {
    "critical-threshold": 70,
    "format": "{icon} {temperatureC}°C",
    "format-critical": "<span color='red'>{icon}</span> {temperatureC}°C",
    "format-icons": [
      "",
      "",
      "",
      "",
      "",
      ""
    ],
    "interval": 5,
    "tooltip": true,
    "on-click": "kitty --title btop -e btop"
  }
}
